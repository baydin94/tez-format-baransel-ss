<?xml version="1.0" encoding="utf-8"?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" class="note" default-locale="tr-TR" demote-non-dropping-particle="never">
  <info>
    <title>Tez Formatı - Baransel (ss. sayfa ve Türkçe "ve")</title>
    <id>http://www.example.com/styles/tez-format-baransel-ss</id>
    <link href="http://www.example.com/styles/tez-format-baransel-ss" rel="self"/>
    <author>
      <name>ChatGPT</name>
    </author>
    <category citation-format="note"/>
    <category field="generic-base"/>
    <updated>2024-06-11T00:00:00+00:00</updated>
  </info>

  <macro name="author">
    <names variable="author">
      <name name-as-sort-order="all" and="text" delimiter=", " delimiter-precedes-last="always">
        <name-part name="family"/>
        <name-part name="given" initialize-with="."/>
      </name>
    </names>
  </macro>

  <macro name="issued">
    <group prefix=" (" suffix=").">
      <date variable="issued">
        <date-part name="year"/>
      </date>
    </group>
  </macro>

  <macro name="title">
    <text variable="title" quotes="true"/>
  </macro>

  <macro name="container-title">
    <text variable="container-title" font-style="italic"/>
  </macro>

  <macro name="pages">
    <group>
      <text value="ss. "/>
      <text variable="page"/>
    </group>
  </macro>

  <citation>
    <layout delimiter="; ">
      <text macro="author"/>
      <text macro="title" prefix=", "/>
      <text macro="container-title" prefix=", "/>
      <text macro="pages" prefix=", "/>
      <text variable="DOI" prefix=", doi: "/>
      <text macro="issued"/>
    </layout>
  </citation>

  <bibliography>
    <layout suffix=".">
      <text macro="author"/>
      <text macro="title" prefix=", "/>
      <text macro="container-title" prefix=", "/>
      <text macro="pages" prefix=", "/>
      <text variable="DOI" prefix=", doi: "/>
      <text macro="issued"/>
    </layout>
  </bibliography>
</style>
